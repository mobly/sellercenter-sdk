{
    "name": "mobly/sellercenter-sdk",
    "description": "SellerCenter PHP-SDK",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "homepage": "https://bitbucket.org/mobly/sellercenter-sdk",
    "support": {
        "issues": "https://bitbucket.org/mobly/sellercenter-sdk/issues"
    },
    "authors": [
        {
            "name": "Mobly Developers",
            "email": "dev@mobly.com.br",
            "homepage": "http://www.mobly.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "SellerCenter\\SDK\\": "src/"
        }
    },
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/command": "0.7.*",
        "guzzlehttp/guzzle-services": "0.*",
        "doctrine/collections": "1.*",
        "zendframework/zend-uri": "2.*",
        "mtdowling/jmespath.php": "~1.1",
        "jms/serializer": "0.16.*",
        "myclabs/php-enum": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpmd/phpmd": "1.*",
        "squizlabs/php_codesniffer": "1.*",
        "symfony/console": "~2.6",
        "zendframework/zend-code": "2.*"
    }
}
