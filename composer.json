{
    "name": "mobly/sellercenter-sdk",
    "description": "SellerCenter PHP-SDK",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "homepage": "https://github.com/mobly/sellercenter-sdk",
    "support": {
        "issues": "https://github.com/mobly/sellercenter-sdk/issues"
    },
    "authors": [
        {
            "name": "Mobly Developers",
            "email": "dev@mobly.com.br",
            "homepage": "http://www.mobly.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "SellerCenter\\SDK\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/command": "~1.0",
        "guzzlehttp/guzzle-services": "0.*",
        "doctrine/collections": "1.*",
        "zendframework/zend-uri": "2.*",
        "mtdowling/jmespath.php": "~2.0",
        "jms/serializer": "~1.0",
        "myclabs/php-enum": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpmd/phpmd": "~2.0",
        "squizlabs/php_codesniffer": "~2.0",
        "symfony/console": "~2.6",
        "zendframework/zend-code": "~2.0",
        "phplint/phplint": "~0.0",
        "phploc/phploc": "~2.0",
        "pdepend/pdepend": "~2.0",
        "sebastian/phpcpd": "~2.0",
        "theseer/phpdox": "0.7.*",
        "satooshi/php-coveralls": "0.6.*"
    }
}
